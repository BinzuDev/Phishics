[sub_resource type="GDScript" id="GDScript_lv8kq"]
script/source = "@tool
extends ScrollContainer

##Allows you to scroll in the editor
@export var autoScroll : int = 0


# Called when the node enters the scene tree for the first time.
func _ready():
	if !Engine.is_editor_hint():
		scroll_vertical = 0
	else:
		scroll_vertical = -500


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta):
	if Engine.is_editor_hint():
		scroll_vertical += autoScroll
"

